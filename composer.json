{
    "name": "qdequippe/oauth2-wave",
    "description": "Wave OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Quentin Dequippe",
            "email": "quentin@dequippe.tech"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "authorization",
        "authentication",
        "idp",
        "identity",
        "sso",
        "single sign on",
        "Wave"
    ],
    "require": {
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "ext-json": "*",
        "phpunit/phpunit": "^9.1",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Qdequippe\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qdequippe\\OAuth2\\Client\\Test\\": "tests/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
